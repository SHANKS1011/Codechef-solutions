#include <iostream>
using namespace std;

int main() {
	int T=0;
	cin >> T;
	while(T--)
	{
	    int a=0,b=0,c=0,d=0;
	    cin >> a >> b >> c >> d;
	    if(a==b && c==d)
	    {
	        cout << "YES\n";
	    }
	    else if(b==c && a==d)
	    {
	        cout << "YES\n";
	    }
	    else if(a==c && b==d)
	    {
	        cout << "YES\n";
	    }
	    else
	        cout << "NO\n";
	    
	}
	return 0;
}
