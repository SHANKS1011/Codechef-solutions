#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	int a[n];
	for(int i=0; i<n; i++)
	{
	    cin >> a[i];
	}
	int odd=0,even=0;
	for(int p=0; p<n; p++)
	{
	    if(a[p]%2==0)
	    {
	        even++;
	    }
	    else
	    {
	        
	    
	     odd++;
	    }
	}
	if(odd>=even)
	{
	    cout << "NOT READY";
	    
	}
	else
	{
	    cout << "READY FOR BATTLE";
	}
	return 0;
}
