#include <iostream>
using namespace std;

int main() {
	int t;
	 cin >> t;
	 while(t>0)
    {
        int n,count=0;
        cin >> n;
        while(n!=0)
        {
           if(n>=100)
           {
               count++;
               n=n-100;
           }
           else if(n>=50&&n<100)
           {
               count++;
               n=n-50;
           }
           else if(n>=10&&n<50)
           {
               count++;
               n=n-10;
           }
           else if(n>=5&&n<10)
           {
               count++;
               n=n-5;
           }
           else if(n>=2&&n<5)
           {
               count++;
               n=n-2;
           }
           else if(n==1)
           {
               count++;
               n=n-1;
           }
           
        }
        cout << count <<endl;
        t--;
    }
	 return 0;
}
