#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,k,i,a,b;
    cin>>t;
    while(t--)
    {
        cin>>n;
        k=0;
        for(i=0;i<n;i++)
        {
            cin>>a>>b;
            if(abs(a-b)>5)
                k++;
        }
        cout<<k<<endl;
    }
	return 0;
}
