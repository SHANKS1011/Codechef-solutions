#include <bits/stdc++.h>
using namespace std;

int main() 
{
    long long int t,sum,i;
    string s;
    cin>>t;
    while(t--)
    {   
        sum=0;
        i=0;
        cin>>s;
        while(s[i]!='\0')
        {
            if(isdigit(s[i]))
            sum+=s[i]-48;
            
            i++;
        }
        cout<<sum<<endl;
        
    }
	return 0;
}
