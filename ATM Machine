#include <iostream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t>0)
	{
	    int n,k;
	    cin >> n >> k;
	    int a[n];
	    for(int i=0; i<n; i++)
	    {
	        cin >> a[i];
	    }
	    
	    for(int j=0; j<n; j++)
	    {
	        if(k-a[j]>=0)
	        {
	            cout << "1";
	            k=k-a[j];
	        }
	        else
	        {
	            cout << "0";
	        }
	    }
	    cout << endl;
	    t--;
	}
	return 0;
}
