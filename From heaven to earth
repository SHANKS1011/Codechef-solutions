#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t>0)
	{
	    
	    float n,v1,v2;
	    cin >> n>> v1>> v2;
	    if(((sqrt(2)*n)/v1)<(2*n/v2))
	    {
	        cout <<"Stairs"<< endl;
	    }
	    else 
	    {
	        cout <<"Elevator"<< endl;
	    }
	    t--;
	}
	return 0;
}
